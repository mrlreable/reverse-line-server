# -*- MakeFile -*-

all: main client

main: main.o
	gcc main.o -o main -l netinet/in.h stdlib.h string.h sys/socket.h sys/types.h

main.o: main.c
	gcc -c main.c

client: client.o
	gcc client.o -o client -l netdb.h stdlib.h string.h sys/socket.h

client.o: client.c
	gcc -c client.c

clean:
	rm *.o main client
